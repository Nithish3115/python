 // Online C compiler to run C program online
#include <stdio.h>

int main() {
     int arr[]={8, 2, 3, 9, 5};
     int size=sizeof(arr)/sizeof(arr[0]);
     for(int i=1;i<size;i++){
         if(arr[i]>arr[i+1] && arr[i]>arr[i-1]){
             printf("%d\n",arr[i]);
         }
         
     }
     if(arr[0]>arr[1]){
         printf("%d\n",arr[0]);
     }
     if(arr[size-1]>arr[size-2]){
         printf("%d\n",arr[size-1]);
     }
     
     
     
     
 
    return 0;
}