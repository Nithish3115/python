  
// left rotation
 #include <stdio.h>
 

int main() {
      
     
      
      int arr[]={3, -5, 2, -8, -1};
      int n=sizeof(arr)/sizeof(arr[0]);
       int sum=0;
      for (int i=0;i<n;i++){
          sum=0;
           
          for (int j=i+1;j<n;j++){
              
              if(arr[i]>arr[j]){
                  sum++;
              }
          }
            
                      printf("%d\n",sum);

      }
       
       
    

    return 0;
}


 
